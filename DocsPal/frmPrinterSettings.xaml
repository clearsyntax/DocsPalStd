<Window x:Class="DocWriter.frmPrinterSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Printer Settings" Height="232" Width="436" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBlock x:Name="tblockAppDefPrinter"  HorizontalAlignment="Left" Height="33" Margin="121,9,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="245"/>
        <Label Content="App Default Printer:" HorizontalAlignment="Left" Height="30" Margin="2,9,0,0" VerticalAlignment="Top" Width="114"/>
        <Label Content="Available Printer(s):" HorizontalAlignment="Left" Height="31" Margin="2,48,0,0" VerticalAlignment="Top" Width="114"/>
        <ComboBox x:Name="cmbPrinterList" IsEditable="True"  HorizontalAlignment="Left" Height="31" Margin="121,47,0,0" VerticalAlignment="Top" Width="248"/>
        <Button x:Name="btnExit" Content="Cancel" HorizontalAlignment="Left" Height="35" Margin="359,145,0,0" VerticalAlignment="Top" Width="57" Click="btnExit_Click"/>
        <Button x:Name="btnSave" Content="Set" HorizontalAlignment="Left" Height="35" Margin="374,47,0,0" VerticalAlignment="Top" Width="42" Click="btnSave_Click"/>

    </Grid>
</Window>
